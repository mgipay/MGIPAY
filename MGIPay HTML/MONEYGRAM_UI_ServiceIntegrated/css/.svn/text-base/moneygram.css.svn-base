/*** Toggle end ***/

/*** FOOTER ***/
.footer {
	background: #303030;
	color: #999999;
	height: 70px;
	width: 100%;
	margin-top: 5%;
	font-size: 12px
}
.footer div {
	width: 1004px;
	margin: auto;
	padding-top: 16px
}
.footer div a {
	color: #999;
	border-left: 1px solid #999;
	float: right;
	padding: 0 10px
}
.footer span {
	float: left
}
.footer div p {
	clear: both;
	padding-top: 12px;
}
